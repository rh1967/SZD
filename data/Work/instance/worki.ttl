    <https://dhplus.sbg.ac.at/ontology#identifiedBy> [
        #number
        a <https://dhplus.sbg.ac.at/ontology#Local> ;
        rdf:value "NN"^^xsd:string ;
        <https://dhplus.sbg.ac.at/ontology#source> [
            #ISIL e.g. AT-OBV
            a <https://dhplus.sbg.ac.at/ontology#Source> ;
            rdfs:label "NN"^^xsd:string ;
        ] ;
    ] ;

    #title
    #MARC 245
    <https://dhplus.sbg.ac.at/ontology#title> [
         a <https://dhplus.sbg.ac.at/ontology#Title> ;
     #MARC summary of $afgknps
         rdfs:label "NN"^^xsd:string ;
         bflc:titleSortKey "NN"^^xsd:string ;
     #MARC $a
         <https://dhplus.sbg.ac.at/ontology#mainTitle> "NN"^^xsd:string ;
     #MARC $b
         <https://dhplus.sbg.ac.at/ontology#subtitle> "NN"^^xsd:string ;
     #MARC $n
         <https://dhplus.sbg.ac.at/ontology#partNumber> "NN"^^xsd:string ;
     #MARC $p
         <https://dhplus.sbg.ac.at/ontology#partName> "NN"^^xsd:string ;
     ] ;

    #Production, Publication, Distribution, Manufacture, and Copyright Notice
    #MARC 264 ind2=1
    dhpluso:provisionActivity [
        a dhpluso:ProvisionActivity, dhpluso:Publication ;
        #MARC $a
        dhpluso:place [
            #place of production, publication, distribution, manufacture
            a dhpluso:Place ;
            #ATTENTION: rdf:value instead of rdfs:label in bibframe
            rdf:value "NN"^^xsd:string ;   #SDZMSK: history/origin/origPlace
        ] ;
        #MARC $b
        #Name of producer, publisher, distributor, manufacturer
        dhpluso:agent [
            #publisher
            a dhpluso:Agent ;
            #ATTENTION: rdf:value instead of rdfs:label in bibframe
            rdf:value "NN"^^xsd:string ;
        ] ;
        #MARC $c
        #date of production, publication, distribution, manufacture, or copyright notice
        dhpluso:date "NN" ;

    ] ;
    #MARC 264 ind2=0
    dhpluso:provisionActivity [
        a dhpluso:Production , dhpluso:ProvisionActivity ;
        #MARC $c
        #date of production, publication, distribution, manufacture, or copyright notice
        dhpluso:date  "NN" ;    #/SDMSK: /history/origin/origDate
        dhpluso:P_szd_dateOfPublication "NN" ;  #/SDMSK: /history/origin/origDate@when  #szd:dateOfPublication
    ] ;
    #MARC 264 summary of $abc
    <https://dhplus.sbg.ac.at/ontology#provisionActivityStatement> "NN"^^xsd:string ;   #only SDZBIB
    #statement of responsibility
    #MARC 245 $c
    <https://dhplus.sbg.ac.at/ontology#responsibilityStatement> "NN"^^xsd:string ;
    #edition
    #MARC 250 $a
    <https://dhplus.sbg.ac.at/ontology#editionStatement> "NN"^^xsd:string ;
    #incipit
    #MARC 290 $a
    #no Bibframe prop.
    dhpluso:P_szd_incipit [ #//msContents/msItem/incipit    #szd:incipit
        rdfs:label  "Incipit"@de, "Incipit"@en ;
        rdf:value "NN"@de, "NN"@en ;
    ] ;
    #physical description
    #MARC 300
    #extent $a
    <https://dhplus.sbg.ac.at/ontology#extent> [
        a dhpluso:Extent ;
        #ATTENTION: rdf:value instead of rdfs:label in bibframe
        rdf:value "NN"@de, "NN"@en ;    #SZDBIB: #//extent/measure@type="page"    #szd:page
                                        #SZDMSK: #//extent/span/*   #szd:txt (part of)
    ] ;
    #dimensions $c
    dhpluso:dimensions "NN" ;   #extent/measure@type="format"   #szd:format
    #accompanying material $e
    dhpluso:note [  #//accMat//item@ana="szdg:AdditionalMaterial"/desc   #szd:additionalMaterial
        a dhpluso:Note ;
        dhpluso:noteType  "Accompanying materials" ;
        #ATTENTION: rdf:value instead of rdfs:label in bibframe
        rdf:value "NN"@de, "NN"@en;
    ] ,
    #accompanying material $e
    #ATTENTION: difference "additionalMaterial" - "enclosure" is not modelled in MARC 21
    [   #//accMat//item@ana="szdg:Enclosures"/desc   #szd:enclosures
        a dhpluso:Note ;
        dhpluso:noteType  "Enclosure" ;
        #ATTENTION: rdf:value instead of rdfs:label in bibframe
        rdf:value "NN"@de, "NN"@en;
    ] ;
    #SZD: Umfang/Einband - Physical Description
    dhpluso:P_szd_extentadd [
        rdfs:label  "Umfang/Einband"@de, "Physical Description"@en ;
        rdf:value "NN"@de, "NN"@en;   #e.g. illustriert   #//list@type="extent"/item/span
    ] ;
    #SZDMSK: szd:objecttyp is part of szd:extent
    dhpluso:P_szd_objecttyp "NN"@de, "NN"@en ;  #//extent/span/term@type="objecttyp"    #szd:objecttyp
    dhpluso:P_szd_text [
        rdfs:label  "Umfang/Einband"@de, "Physical Description"@en ;
        rdf:value "NN"@de, "NN"@en; #SZDBIB: #//extent/* + //list@type="extent"//*   #szd:text #= extent + extentadd + dimensions + binding (see item)
                                    #SZDMSK: ##//extent//*  #szd:txt
    ] ;
    #glossar
    dhpluso:P_szd_glossar szdv:nnnn ;

    #in series
    #MARC 8xx
    <https://dhplus.sbg.ac.at/ontology#hasSeries> <http://dhplus.sbg.ac.at/szd/instance/17728dbc-bc9c-11ea-b3de-0242ac130004/Instance8xx> ;

    <https://dhplus.sbg.ac.at/ontology#instanceOf> <http://dhplus.sbg.ac.at/szd/instance/17728dbc-bc9c-11ea-b3de-0242ac130004/Work> ;
    <https://dhplus.sbg.ac.at/ontology#hasItem> <http://dhplus.sbg.ac.at/szd/instance/17728dbc-bc9c-11ea-b3de-0242ac130004/Item> ;
.
