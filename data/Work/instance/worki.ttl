#work
<http://dhplus.sbg.ac.at/szd/instance/17728dbc-bc9c-11ea-b3de-0242ac130004/Work>
    a <https://dhplus.sbg.ac.at/ontology#Work> ;

    ##author, title - short
    rdfs:label "NN"^^xsd:string ;

    ##adminMetadata
    #MARC 001. 003, 016
    <https://dhplus.sbg.ac.at/ontology#adminMetadata> [
        a <https://dhplus.sbg.ac.at/ontology#AdminMetadata> ;
        #ATTENTION: DNB uses 001 and 016, ALMA uses 035 (under instance)
        #control number
        #MARC 001
        <https://dhplus.sbg.ac.at/ontology#identifiedBy> [
            a <https://dhplus.sbg.ac.at/ontology#Local> ;
            rdf:value "NN"^^xsd:string ;
        ] ;
        #national bibliographic agency control number
        #MARC 016
        <https://dhplus.sbg.ac.at/ontology#identifiedBy> [

            a <https://dhplus.sbg.ac.at/ontology#Local> ;
            #record control number
            #Marc 016 $a
            rdf:value "NN"^^xsd:string ;
            <https://dhplus.sbg.ac.at/ontology#source> [
                #source e.g. ISIL
                #Marc 016 $2
                a <https://dhplus.sbg.ac.at/ontology#Source> ;
                rdfs:label "NN"^^xsd:string ;
            ]
        ] ;
        #control number identifier e.g. ISIL
        #MARC 003
        <https://dhplus.sbg.ac.at/ontology#source> [
            a <https://dhplus.sbg.ac.at/ontology#Source> ;
            <https://dhplus.sbg.ac.at/ontology#code> "NN"^^xsd:string ;
        ] ;
        #date and time of latest transaction
        #MARC 005
        <https://dhplus.sbg.ac.at/ontology#changeDate> "0001-01-01T00:00:00"^^xsd:dateTime ;
        #date of creation
        #MARC 008, character 0-5
        <https://dhplus.sbg.ac.at/ontology#creationDate> "0001-01-01"^^xsd:date ;
    ] ;

    ##contribution
    #MARC 100, 500, 700
    <https://dhplus.sbg.ac.at/ontology#contribution> [
        a <https://dhplus.sbg.ac.at/ontology#Contribution> ;
        #Main Entry-Personal Name
        #MARC 100
        #OR
        #General Note - Personenname (DNB)
        #MARC 500
        #ATTENTION: Record Control Number (GND) in 500 is not converted by LoC
        #OR
        #Added Entry-Personal Name
        #MARC 700

        #relation to dhplus:Person dataset
        <https://dhplus.sbg.ac.at/ontology#agent> <URI> ;

        #Relator term
        #MARC $e
        <https://dhplus.sbg.ac.at/ontology#role> [
            a <https://dhplus.sbg.ac.at/ontology#Role> ;
            rdfs:label "NN"^^xsd:string ;
        ] ;
        #OR
        #author
        <https://dhplus.sbg.ac.at/ontology#role> <http://id.loc.gov/vocabulary/relators/aut> ;
        #OR
        #contributor
        <https://dhplus.sbg.ac.at/ontology#role> <http://id.loc.gov/vocabulary/relators/ctb> ;
        #OR
        #editor
        <https://dhplus.sbg.ac.at/ontology#role> <http://id.loc.gov/vocabulary/relators/edt> ;

        dhpluso:P_szd_relationToPerson <URI> ;  #link to person object
                                                #//profileDesc/keywords/term@type="person"  #szd:relationToPerson

    ] ;

    ##language
    #MARC 041 $a
    <https://dhplus.sbg.ac.at/ontology#language> <http://id.loc.gov/vocabulary/languages/nnnn> ;

    ##title
    #MARC 245
    <https://dhplus.sbg.ac.at/ontology#title> [
        a <https://dhplus.sbg.ac.at/ontology#Title> ;
        #MARC summary of $afgknps
        rdfs:label "NN"^^xsd:string ;
        #string for sorting
        bflc:titleSortKey "NN"^^xsd:string ;
        #MARC $a
        <https://dhplus.sbg.ac.at/ontology#mainTitle> "NN"^^xsd:string ;
        #MARC $b
        <https://dhplus.sbg.ac.at/ontology#subtitle> "NN"^^xsd:string ;
        #MARC $n
        <https://dhplus.sbg.ac.at/ontology#partNumber> "NN"^^xsd:string ;
        #MARC $p
        <https://dhplus.sbg.ac.at/ontology#partName> "NN"^^xsd:string ;
    ] ;
    ##content type
    #MARC 336 $b
    dhpluso:content <http://id.loc.gov/vocabulary/contentTypes/nnnn> ;
    ##host item entry e.g. monography -> series
    #MARC 773
    <https://dhplus.sbg.ac.at/ontology#partOf> <http://dhplus.sbg.ac.at/szd/instance/17728dbc-bc9c-11ea-b3de-0242ac130004/Work773> ;

    <https://dhplus.sbg.ac.at/ontology#hasInstance> <http://dhplus.sbg.ac.at/szd/instance/17728dbc-bc9c-11ea-b3de-0242ac130004/Instance> ;
.
##host item work
#MARC 773
<http://dhplus.sbg.ac.at/szd/instance/17728dbc-bc9c-11ea-b3de-0242ac130004/Work773>
    a <https://dhplus.sbg.ac.at/ontology#Work> ;
    #host item entry
    #MARC 773
        <https://dhplus.sbg.ac.at/ontology#identifiedBy> [
        #record control number
        #MARC 773 $w
        a <https://dhplus.sbg.ac.at/ontology#Identifier> ;
        #number
        rdf:value "NN"^^xsd:string ;
        <https://dhplus.sbg.ac.at/ontology#source> [
            #ISIL e.g. AT-OBV
            a <https://dhplus.sbg.ac.at/ontology#Source> ;
            rdfs:label "NN"^^xsd:string ;
        ]
    ] ;
        <https://dhplus.sbg.ac.at/ontology#hasInstance> <http://dhplus.sbg.ac.at/szd/instance/17728dbc-bc9c-11ea-b3de-0242ac130004/Instance773>
.
##series-work
#MARC 8xx
<http://dhplus.sbg.ac.at/szd/instance/17728dbc-bc9c-11ea-b3de-0242ac130004/Work8xx>
    a <https://dhplus.sbg.ac.at/ontology#Work>, <https://dhplus.sbg.ac.at/ontology#Text> ;
    #series added entry-uniform title
    #MARC 8xx
    rdfs:label "NN"^^xsd:string ;
        <https://dhplus.sbg.ac.at/ontology#title> [
        a <https://dhplus.sbg.ac.at/ontology#Title> ;
        #MARC summary of $afgknps
        rdfs:label "NN"^^xsd:string ;
        #string for sorting
        bflc:titleSortKey "NN"^^xsd:string ;
        #uniform title
        #MARC $a
        <https://dhplus.sbg.ac.at/ontology#mainTitle> "NN"^^xsd:string ;
        bflc:title30MatchKey "NN"^^xsd:string ;
        bflc:title30MarcKey "NN"^^xsd:string ;
    ] ;
        <https://dhplus.sbg.ac.at/ontology#identifiedBy> [
        a <https://dhplus.sbg.ac.at/ontology#Identifier> ;
        rdf:value "NN"^^xsd:string ;
        <https://dhplus.sbg.ac.at/ontology#source> [
            a <https://dhplus.sbg.ac.at/ontology#Source> ;
            rdfs:label "NN"^^xsd:string ;
        ]
    ] ;
    #Control subfield
    #MARC $7
        <https://dhplus.sbg.ac.at/ontology#issuance> [
        a <https://dhplus.sbg.ac.at/ontology#Issuance> ;
        #Bibliographic level
        #MARC $7, second character e.g. s = series
        <https://dhplus.sbg.ac.at/ontology#code> "s" ;
    ] ;
.

#instance
<http://dhplus.sbg.ac.at/szd/instance/17728dbc-bc9c-11ea-b3de-0242ac130004/Instance>
