#item
<http://dhplus.sbg.ac.at/szd/instance/17728dbc-bc9c-11ea-b3de-0242ac130004/Item>
    a   <https://dhplus.sbg.ac.at/ontology#Item> ;

    #ownership and custodial history
    #MARC 561 history $a
    #provenance SZDMSK
    dhpluso:custodialHistory [
        rdfs:label "Provenienz"@de, "Provenance"@en ;
        rdf:value "NN" ;    # //provenance/ab   #szd:provenance
    ] ,
    #provenance SZDBIB
    [   # //provenance/orgName  # szd:provenance
        dhpluso:P_szd_glossar szdv:LaterOwner ;
        rdfs:label "Nachbesitzer/in"@de, "Later Owner"@en ;
        rdf:value "NN" ;
    ] ;

    #immediate source of acquisition note MARC 541 source of acquisition $a
    dhpluso:immediateAcquisition [   # //acquisition/ab # szd:acquired
        a dhpluso:ImmediateAcquisition ;
        rdfs:label  "Erwerbung"@de, "Acquisition"@en ;
        rdf:value "NN" ;
    ] ;

    #binding information MARC 563 binding note $a
    #ATTENTION 2 types of binding description one as instance one as item
    #SZD Work: only binding as item
    dhpluso:note [  #   //bindingDesc/binding/ab    # szd:binding
        a dhpluso:Note ;
        dhpluso:noteType  "binding" ;
        rdfs:label  "Umfang/Einband"@de, "Physical Description"@en ;
        #ATTENTION: rdf:value instead of rdfs:label in bibframe
        rdf:value "NN" ;
    ] ;

    dhpluso:P_szd_note [    #SZDBIB://msContents/msItem/note   #SZDMSK://notesStmt/note
        rdfs:label "Hinweis"@de, "Comment"@en ;
        rdf:value "NN"@de, "NN"@en ;
    ] ;

    dhpluso:P_szd_location [
        a dhpluso:E_szd_Location ;
        rdfs:label "Heutiger Standort"@de, "Current Location"@en ;
        dhpluso:P_szd_repository "NN" ; #//msDesc/msIdentifier/repository
        dhpluso:P_szd_settlement <URI> ;  #link to place object #//msDesc/msIdentifier/settlement
        dhpluso:P_szd_signature "NN" ;  #//msDesc/msIdentifier/idno   #szd:signature
    ] ;

    dhpluso:P_szd_identifyingInscription [
        rdfs:label  "Aufschrift"@de, "Identifying Inscription"@en ;
        rdf:value "NN"@de, "NN"@en ;    #SZDMSK: //msContents/textLang/msItem/docEdition    #szd:identifyingInscription
    ] ;

    dhpluso:P_szd_secretarialHand [
        rdfs:label  "Schreiberhand"@de, "Secretarial Hand"@en ;
        rdf:value "NN"@de, "NN"@en ;    #SZDMSK: //handDesc/ab  #szd:secretarialHand
    ] ;
    dhpluso:P_szd_writingInstrument [
        rdfs:label  "Schreibstoff"@de, "Writing Instrument"@en ;
        rdf:value "NN"@de, "NN"@en ;    #SZDMSK: //support/material@ana="szdg:WritingInstrument"  #szd:writingInstrument
    ] ;
    dhpluso:P_szd_writingMaterial [
        rdfs:label  "Beschreibstoff"@de, "Writing Material"@en ;
        rdf:value "NN"@de, "NN"@en ;    #SZDMSK: //support/material@ana="szdg:WritingMaterial"  #szd:writingMaterial
    ] ;

