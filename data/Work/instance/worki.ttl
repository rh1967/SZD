    ] ;
    bf:identifiedBy [
        a bf:Local ;
        rdf:value "NN"^^xsd:string ;
        bf:source [
            a bf:Source ;
            rdfs:label "NN"^^xsd:string ;
        ] ;
    ] ;

    #title statement
    #MARC 245
    bf:title [
         a bf:Title ;
     #MARC summary of $afgknps
         rdfs:label "NN"^^xsd:string ;
         bflc:titleSortKey "NN"^^xsd:string ;
     #MARC $a
         bf:mainTitle "NN"^^xsd:string ;
     #MARC $b
         bf:subtitle "NN"^^xsd:string ;
     #MARC $n
         bf:partNumber "NN"^^xsd:string ;
     #MARC $p
         bf:partName "NN"^^xsd:string ;
     ] ;

    #Production, Publication, Distribution, Manufacture, and Copyright Notice
    #MARC 264 ind2=1
    bf:provisionActivity [
        a bf:ProvisionActivity, bf:Publication ;
        #MARC $a
        bf:place [
            #place of production, publication, distribution, manufacture
            a bf:Place ;
            rdfs:label "NN"^^xsd:string ;
        ] ;
        #MARC $b
        #Name of producer, publisher, distributor, manufacturer
        bf:agent [
            #publisher
            a bf:Agent ;
            rdfs:label "NN"^^xsd:string ;
        ] ;
        #MARC $c
        #date of production, publication, distribution, manufacture, or copyright notice
        bf:date "0001"^^xsd:date ;
        #OR
        bf:date "NN" ;
    ] ;
    #MARC 264 ind2=0
    bf:provisionActivity [
        a bf:Production , bf:ProvisionActivity ;
        #MARC $c
        #date of production, publication, distribution, manufacture, or copyright notice
        bf:date  "NN" ;
    ] ;
    #MARC 264 summary of $abc
    bf:provisionActivityStatement "NN"^^xsd:string ;
    #statement of responsibility
    #MARC 245 $c
    bf:responsibilityStatement "NN"^^xsd:string ;
    #edition
    #MARC 250 $a
    bf:editionStatement "NN"^^xsd:string ;
    #physical description
    #MARC 300
    #extent
    #$a
    bf:extent [
        a bf:Extent ;
        rdfs:label  "NN" ;
    ] ;
    #dimensions
    #$c
    bf:dimensions "NN" ;
    #accompanying material
    #$e
    bf:note [
        a bf:Note ;
        rdfs:label   "NN" ;
        bf:noteType  "Accompanying materials"
    ] ;
    #issuance e.g. mono
    bf:issuance <http://id.loc.gov/vocabulary/issuance/nnnn> ;
    #General Note
    #MARC 500 $a
    bf:note [
        a bf:Note ;
        rdfs:label "NN"
    ] ;
    #restrictions on access note
    #MARC 506
    #terms governing access
    #$a
    bf:usageAndAccessPolicy [
        a bf:UsageAndAccessPolicy ;
        rdfs:label  "Nur im LAS benutzbar"
    ] ;
    #in series
    #MARC 8xx
    bf:hasSeries [
        #series-instance
        a bf:Instance ;
        #series statement
        #MARC $a
        rdfs:label "NN"^^xsd:string ;
        #volume/sequential designation
        #MARC $v
        bf:seriesEnumeration "NN"^^xsd:string ;
        #MARC summary of $av
        bf:seriesStatement "NN"^^xsd:string ;

        bf:instanceOf <http://example.org/455751595#Work8xx> ;
    ] ;

